<%- include('../includes/trainerhead') %>
<link rel="stylesheet" type="text/css" href="/css/trainer.css" />
    
</head>
<body>
    


    <main>
       

    <section>
        <div class="row">
            <%- include('../includes/trainernav.ejs', {trainername:trainers.fname}) %>

       
            <div  class="col-xl-8 main-dashboard">
               <h1>My Client List</h1>
            
        <% if(!users.length){ %>
            <h3>No Client Enrolled</h3>
        <% }else{ %>
            <% for (user of users) { %>

                
                    <div class="card-grid">
                        <div class="row-grid">
                          <span><img src="/asset/icons/user-gold.png" alt=""></span>
                                <span><%= user.user[0].fname %></span>
                                <span><%= user.user[0].address %></span>
                                <span><%= user.user[0].email %></span>
                        </div>
                            
                              <div class="event">
                                    <a href="/trainer/appointment/<%= user.user[0]._id %>">Appointments</a> | <a href="/trainer/note/<%= user.user[0]._id %>">Note</a>
                              </div>
                        
                    </div>
                    
               

                 <% } %>
            <% } %>

          
                  </div>
                </div>
              
    </section>
    </main>




<%- include('../includes/footer.ejs') %>